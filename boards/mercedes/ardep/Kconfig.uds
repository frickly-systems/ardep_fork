# enable UDS module per default

config UDS
	default y

config ISO14229
	default y

config CAN
	default y

# Disable zephyr iso-tp module as we use an external module
config ISOTP
	default n

config FLASH
    default y

config STREAM_FLASH
    default y

config IMG_MANAGER
    default y

config IMG_ERASE_PROGRESSIVELY
    default y

config REBOOT
    default y
