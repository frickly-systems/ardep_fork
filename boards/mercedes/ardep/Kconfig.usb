# Set USB device descriptors for ARDEP board

config USB_DEVICE_PRODUCT
	default "ARDEP Board"
config USB_DEVICE_VID
	default 0x25E1
config USB_DEVICE_PID
	default 0x1B1E
config USB_DEVICE_DFU_PID
	default 0x1B1E