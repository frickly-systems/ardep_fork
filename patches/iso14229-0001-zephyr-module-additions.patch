From e2405f1dc1dd7579c502320927ac96e6ad51cf5a Mon Sep 17 00:00:00 2001
From: Mark Oude Elberink <mark.oude.elberink@frickly.systems>
Date: Thu, 14 Aug 2025 13:18:49 +0200
Subject: [PATCH] zephyr module additions

Signed-off-by: Mark Oude Elberink <mark.oude.elberink@frickly.systems>
---
 src/sys.h          | 4 ++++
 zephyr/module.yaml | 4 ++++
 2 files changed, 8 insertions(+)
 create mode 100644 zephyr/module.yaml

diff --git a/src/sys.h b/src/sys.h
index 8afea10..eb1d114 100644
--- a/src/sys.h
+++ b/src/sys.h
@@ -26,3 +26,7 @@
 #include "sys_win32.h"
 #include "sys_arduino.h"
 #include "sys_esp32.h"
+
+#if UDS_SYS == UDS_SYS_CUSTOM
+#include "sys_custom.h"
+#endif
diff --git a/zephyr/module.yaml b/zephyr/module.yaml
new file mode 100644
index 0000000..672fcfc
--- /dev/null
+++ b/zephyr/module.yaml
@@ -0,0 +1,4 @@
+name: iso14229
+build:
+  cmake-ext: True
+  kconfig-ext: True
